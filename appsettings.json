{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "TempPaths": {
    "Uploads": "temp/uploads",
    "Outputs": "temp/outputs"
  },
  "FileUpload": {
    "MaxFileSizeBytes": 2147483648,
    "MaxFileSizeMB": 2048
  },
  "Compression": {
    "MaxConcurrentJobs": 1,
    "MaxQueueSize": 10,
    "CleanupIntervalMinutes": 5,
    "JobRetentionMinutes": 30
  },
  "FFmpeg": {
    "Path": ""
  }
}
